{
  "name": "function-calling-examples",
  "description": "Several examples of function calling using Google Gemini models in Javascript",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "@google/generative-ai": "^0.14.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "scripts": {
    "prestart": "echo 'Hello, World!'",
    "start:intro": "node -r ts-node/register --env-file=.env 1.intro/index.ts",
    "start:data:single": "node -r ts-node/register --env-file=.env 2.data-structures/single-parameter.ts",
    "start:data:multiple": "node -r ts-node/register --env-file=.env 2.data-structures/multiple-parameter.ts",
    "start:data:list": "node -r ts-node/register --env-file=.env 2.data-structures/list-parameter.ts",
    "start:data:nested": "node -r ts-node/register --env-file=.env 2.data-structures/nested-parameter.ts",
    "start:parallel": "node -r ts-node/register --env-file=.env 3.parallel-function-calling/parallel.ts",
    "start:sql:ddl": "node -r ts-node/register --env-file=.env 4.sql-talk-app/ddl.ts",
    "start:sql:seed": "node -r ts-node/register --env-file=.env 4.sql-talk-app/seed.ts",
    "start:sql": "node -r ts-node/register --env-file=.env 4.sql-talk-app/index.ts"
  },
  "keywords": [
    "google",
    "gemini",
    "function",
    "calling",
    "examples",
    "javascript",
    "typescript",
    "nodejs"
  ],
  "author": "Carlos F Arboleda G <arbofercho@gmail.com>",
  "license": "ISC"
}
